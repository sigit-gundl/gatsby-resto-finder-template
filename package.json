    "cli-glob": "^0.1.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.4",
    "gh-pages": "^3.1.0",
    "prettier": "2.0.5",
    "remark-cli": "^8.0.1",
    "remark-preset-lint-recommended": "^4.0.1",
    "warning": "^4.0.3",
    "write-good": "^1.0.2"
